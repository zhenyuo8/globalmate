{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime", "transform-vue-jsx", "syntax-dynamic-import", ["component", [
    {
      "libraryName": "mint-ui",
      "style": true
    }
  ]]],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node", "syntax-dynamic-import"]
    }
  }
}
